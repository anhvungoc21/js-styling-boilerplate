{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"airbnb",
		"airbnb/hooks"
		// "prettier"
		// "plugin:node/recommended",
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react", 
		"no-autofix"
		// "prettier"
	],
	"rules": {
		"prefer-const": "off",
		"no-autofix/prefer-const": "warn",
		"no-unused-vars": "warn",
		"func-names": "off",
		"import/no-extraneous-dependencies": "off"
		// "prettier/prettier": "warn"
	}
}
